{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "A simple boilerplate for ReactJS",
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "npm run build && npm run start-prod-server",
    "dev": "npm run clean && cross-env SERVE=true NODE_ENV=development webpack serve --progress --color",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --color",
    "build:gh-pages": "npm run clean && cross-env NODE_ENV=gh-pages webpack --progress --color",
    "start-prod-server": "cross-env SERVE=true NODE_ENV=production node server.js",
    "clean": "rimraf ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx && tsc --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rushelex/react-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "typescript",
    "react",
    "boilerplate"
  ],
  "author": "Rushelex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rushelex/react-boilerplate/issues"
  },
  "homepage": "https://github.com/rushelex/react-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.0",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "coveralls": "^3.1.1",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "figlet": "^1.5.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "type-fest": "^0.20.2",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "lodash-es": "^4.17.21",
    "p-min-delay": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-pretty": "^2.2.0",
    "react-router-dom": "^5.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
